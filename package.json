{
    "name": "@augu/eslint-config",
    "description": "ðŸ“œ @augu/eslint-config is Noel's shared configuration for his projects and for Noelware as well",
    "version": "5.0.0",
    "license": "MIT",
    "author": "Noel <cutie@floofy.dev>",
    "repository": "https://github.com/auguwu/eslint-config",
    "packageManager": "yarn@4.0.2",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup --clean --format cjs src/index.ts && tsup --dts --format esm src/index.ts && mv dist/index.d.mts dist/index.d.ts",
        "lint": "bun run scripts/eslint.ts",
        "fmt": "bun run scripts/prettier.ts"
    },
    "peerDependenciesMeta": {
        "@typescript-eslint/eslint-plugin": {
            "optional": true
        },
        "@typescript-eslint/parser": {
            "optional": true
        },
        "eslint-config-prettier": {
            "optional": true
        },
        "eslint-plugin-antfu": {
            "optional": true
        },
        "eslint-plugin-perfectionist": {
            "optional": true
        },
        "eslint-plugin-vue": {
            "optional": true
        },
        "vue-eslint-parser": {
            "optional": true
        }
    },
    "dependencies": {
        "@actions/core": "1.10.1",
        "@noelware/utils": "2.4.0",
        "@typescript-eslint/eslint-plugin": "6.15.0",
        "@typescript-eslint/parser": "6.15.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-antfu": "2.1.1",
        "eslint-plugin-perfectionist": "2.5.0",
        "eslint-plugin-vue": "9.19.2",
        "local-pkg": "0.5.0",
        "vue-eslint-parser": "9.3.2"
    },
    "devDependencies": {
        "@augu/tsconfig": "1.2.0",
        "@types/eslint": "8.56.0",
        "@types/node": "20.10.5",
        "bun-types": "1.0.20",
        "colorette": "2.0.20",
        "eslint": "8.56.0",
        "eslint-formatter-codeframe": "7.32.1",
        "prettier": "3.1.1",
        "tsup": "8.0.1",
        "tsx": "4.7.0",
        "typescript": "5.3.3"
    }
}
